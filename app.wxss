/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.app_input {
  background-color: rgb(255, 255, 255);
  width: 400rpx;
  height: 60rpx;
  text-align: end;
  padding: 0rpx 20rpx;
  letter-spacing: 2rpx;
  border-radius: 10rpx;
  box-shadow: 0rpx 0rpx 4rpx gainsboro;
  font-size: 34rpx;
}

.app_input:hover {
  background-color: rgb(255, 255, 255);
  width: 400rpx;
  height: 60rpx;
  text-align: end;
  padding: 0rpx 20rpx;
  letter-spacing: 2rpx;
  border-radius: 10rpx;
  box-shadow: 0rpx 0rpx 6rpx skyblue;
  font-size: 34rpx;
}

.app_boxShow_error {
  box-shadow: 0 0 8rpx red;
}

.app_boxShow_success {
  box-shadow: 0 0 8rpx skyblue;
}

/* ---------------------------width=750------------------------------------- */

.app_main {
  height: 100%;
  width: 100%;
  background-color: #efeeee;
  overflow-y: scroll;
}

.app_label {
  height: inherit;
  width: 200rpx;
  display: flex;
  align-items: center;
  justify-content: start;
  font-size: 36rpx;
  letter-spacing: 2rpx;
  font-weight: 600;
  /* background-color: saddlebrown; */
}

.app_text_lv1 {
  font-size: 40rpx;
  font-weight: 700;
}

.app_text_lv2 {
  font-size: 40rpx;
  font-weight: 600;
}

.app_text_lv3 {
  font-size: 36rpx;
  font-weight: 600;
}

.app_text_lv4 {
  font-size: 32rpx;
  font-weight: 600;
}

.app_text_lv5 {
  font-size: 30rpx;
  font-weight: 500;
}

.app_text_lv6 {
  font-size: 28rpx;
  font-weight: 500;
}

.app_text_money {
  font-size: 36rpx;
  font-weight: 600;
  color: red;
  letter-spacing: 2rpx;
  padding: 0 10rpx;
}

.app_text_category {
  font-size: 28rpx;
  font-weight: 700;
}

.app_item {
  height: auto;
  width: auto;
  padding: 10rpx 20rpx;
}

.app_flex_center_end {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.app_flex_center_center {
  display: flex;
  justify-content: center;
  align-items: center;
}

.app_flex_center_spaceBetween {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.app_flex_start_spaceBetween {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.app_flex_center_start {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 0 10rpx;
}

.app_flex_start_start {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}

.app_card {
  z-index: 0;
  height: auto;
  width: auto;
  margin: 30rpx 30rpx 30rpx 30rpx;
  padding: 10rpx 10rpx;
  border-radius: 20rpx;
  box-shadow: -12rpx -12rpx 24rpx rgba(255, 255, 255, 1),
                12rpx 12rpx 24rpx rgba(217, 210, 200, 1);
}

.app_show {
  transition: all 0.5s;
  opacity: 1;
}

.app_hidden {
  margin-bottom: 100%;
  transition: all 0.2s;
  opacity: 0;
}

.app_card:hover {
  box-shadow: inset 12px 12rpx 24rpx rgb(211, 208, 205, 1),
                inset -12rpx -12rpx 24rpx rgba(255, 255, 255, 1);
}

.app_avatar {
  height: 120rpx;
  width: 120rpx;
  border-radius: 50%;
}

.app_avatar_big {
}

.app_avatar_small {
  height: 80rpx;
  width: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.app_avatar_smaller {
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
  margin: 0 20rpx;
}

.app_image_status {
  height: 200rpx;
  width: 200rpx;
  border-radius: 50%;
  position: absolute;
  margin-top: 20rpx;
  margin-left: 450rpx;
}

