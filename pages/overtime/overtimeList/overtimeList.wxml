<!--pages/overtime/overtime.wxml-->
<view class="main">

  <searchBox bind:search="search" bind:reset="reset" bind:add="bindShowDialog"></searchBox>
  
  <view style="height:10rpx;">
    <!-- 防止margin-top 垂直方向塌陷，在第一个元素前增加空元素。 -->
  </view>

  <scroll-view scroll-y="{{true}}" style="height:{{windowHeight-170}}rpx;margin-top: 160rpx;">
    <loading position="center" wx:if="{{!inited}}"></loading>
    <loading position="bottom" wx:if="{{loadMore}}"></loading>
    <overTimeCard wx:for="{{targetAcList}}" wx:key="index" vIndex="index" overTimeInfo="{{item}}" cardFrom="overTime"></overTimeCard>
  </scroll-view>
</view>

