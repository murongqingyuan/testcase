/* pages/debit/DptDebit/DptDebit.wxss */

page {
  height: 100%;
}

.movie-container {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.navbar {
  display: flex;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 500;
  width: 100%;
  height: 50px;
  flex-direction: row;
  text-align: center;
  color: #a8a8a8;
  font-size: 15px;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom: 1rpx solid #dfdfdf;
}

.navbar-item {
  flex: 1;
  padding: 26rpx 0px;
}

.navbar-item-active {
  transition: all 0.3s;
  border-bottom: 10rpx solid #494949;
  color: #494949;
}

.movie-content-wrapper {
  padding-top: 50px;
  height: 100%;

}
@keyframes myTY {
  from {
    transform: translateY(1000rpx);
  }

  to {
    transform: translateY(0);
  }
}
